<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.taetae98.wildriftdictionary">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".WildRiftDictionary"
        android:allowBackup="true"
        android:icon="@drawable/icon_app_icon"
        android:label="@string/app_name"
        android:roundIcon="@drawable/icon_app_round_icon"
        android:supportsRtl="true"
        android:theme="@style/Theme.WildRiftDictionary"
        android:usesCleartextTraffic="true">
        <activity android:name=".activity.ChampionActivity">

        </activity>
        <activity android:name=".activity.WebViewActivity" />
        <activity android:name=".activity.MainActivity" />
        <activity
            android:theme="@style/Theme.WildRiftDictionary.Splash"
            android:name=".activity.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>